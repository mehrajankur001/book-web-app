<h1 class="">Search</h1>
<form action="/authors" method="get">
    <input type="text"
     class="box" name="name"
     autocomplete="off"
     value=<%= searchOptions.name %> >
    <button type="submit">Search</button>
</form>
<br>
<div class="box">
<% authors.forEach((author)=> { %>
    <div class="item">
        <h3><%= author.name %></h3>
    </div>
<% }) %>
</div>
